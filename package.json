{
    "name": "react-curriculum-app",
    "version": "1.0.0",
    "description": "An awesome application to handle some curriculum vitaes.",
    "dependencies": {
        "@types/node": "20.11.4",
        "@types/react": "18.2.48",
        "@types/react-dom": "18.2.18",
        "@types/react-router-dom": "^5.1.2",
        "bootstrap": "^5.3.3",
        "bootstrap-icons": "^1.11.3",
        "date-fns": "^4.1.0",
        "history": "^5.3.0",
        "jwt-decode": "^4.0.0",
        "react": "^18.2.0",
        "react-bootstrap-icons": "^1.10.3",
        "react-datepicker": "^7.4.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^5.1.2",
        "react-scripts": "^5.0.1",
        "react-to-print": "^3.0.1",
        "sass": "^1.79.4",
        "typescript": "3.2.1"
    },
    "scripts": {
        "start": "react-scripts start",
        "start:api": "json-server --watch src/models/db.json --port=3001 --delay=2500",
        "build": "react-scripts build"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "configurations": [
        {
            "type": "msedge",
            "request": "launch",
            "name": "Launch Edge against localhost",
            "url": "http://localhost:3000",
            "webRoot": "${workspaceFolder}"
        }
    ]
}
